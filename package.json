{
  "name": "persistent-tunnel",
  "version": "3.0.0",
  "description": "HTTP Agent for tunneling proxies with persistent sockets",
  "type": "module",
  "main": "lib/index.js",
  "typing": "lib/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "balena-lint src/ test/",
    "lint-fix": "balena-lint --fix src/ test/",
    "check": "tsc --pretty --noEmit --diagnostics --project .",
    "build": "tsc --pretty --project ./tsconfig.dist.json",
    "pretest": "npm run build",
    "test": "mocha --extension ts test",
    "posttest": "npm run check",
    "prepublish": "require-npm4-to-publish",
    "prepare": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balena-io-modules/persistent-tunnel.git"
  },
  "keywords": [
    "http",
    "agent",
    "proxy",
    "tunnel"
  ],
  "author": "Kostas Lekkas <kostas@balena.io>",
  "contributors": [
    "Will Boyce <will@balena.io>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/balena-io-modules/persistent-tunnel/issues"
  },
  "homepage": "https://github.com/balena-io-modules/persistent-tunnel#readme",
  "devDependencies": {
    "@balena/lint": "^9.3.13",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.11",
    "chai": "^6.2.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mocha": "^11.7.5",
    "node-tunnel": "^2.1.2",
    "require-npm4-to-publish": "^1.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "^22.22.0 || >=24.13.1",
    "npm": ">=10.9.4"
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ]
  },
  "versionist": {
    "publishedAt": "2026-02-24T16:00:05.666Z"
  }
}
