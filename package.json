{
  "name": "persistent-tunnel",
  "version": "2.2.1",
  "description": "HTTP Agent for tunneling proxies with persistent sockets",
  "main": "lib/index.js",
  "typing": "lib/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "balena-lint --typescript src/ test/ && balena-lint test/",
    "lint-fix": "balena-lint --typescript --fix src/ test/",
    "check": "tsc --pretty --noEmit --diagnostics --project .",
    "build": "tsc --pretty --project ./tsconfig.dist.json",
    "test": "mocha --require ts-node/register/type-check test/*",
    "prepublish": "require-npm4-to-publish",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balena-io-modules/persistent-tunnel.git"
  },
  "keywords": [
    "http",
    "agent",
    "proxy",
    "tunnel"
  ],
  "author": "Kostas Lekkas <kostas@balena.io>",
  "contributors": [
    "Will Boyce <will@balena.io>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/balena-io-modules/persistent-tunnel/issues"
  },
  "homepage": "https://github.com/balena-io-modules/persistent-tunnel#readme",
  "devDependencies": {
    "@balena/lint": "^5.4.2",
    "@types/bluebird": "^3.5.26",
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/node": "^22.19.11",
    "bluebird": "^3.5.3",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.11",
    "mocha": "^6.0.2",
    "node-tunnel": "^2.1.1",
    "require-npm4-to-publish": "^1.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "typed-error": "^3.1.0"
  },
  "engines": {
    "node": "^22.22.0 || >=24.13.1",
    "npm": ">=10.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --typescript --fix",
      "git add"
    ]
  }
}
